<j:jelly xmlns:j="jelly:core"
         xmlns:t="/lib/hudson">
    <j:if test="${!empty it.deployedGradleArtifacts}">
        <t:summary icon="${it.iconFileName}"> Deployed Gradle Artifacts
            <ul>
                <j:forEach var="artifact" items="${it.deployedGradleArtifacts}">
                    <li><a href="${artifact.url}">${artifact.name}</a></li>
                </j:forEach>
            </ul>
        </t:summary>
    </j:if>
</j:jelly>